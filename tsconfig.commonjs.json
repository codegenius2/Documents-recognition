{
  "compilerOptions": {
    "rootDir": "src",
    "target": "es5",
    "module": "commonjs",
    "outDir": "dist/cjs-es5",
    "inlineSourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "lib": [
      "es5",
      "es2015.promise"
    ]
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}
